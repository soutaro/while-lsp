<?php

function Abs1($x) {
  if ($x < 0) {
    return 0 - $x;
  } else {
    return $x;
  }
}

function GCD($x, $y) {
  if ($x == 0) {
    return $y;
  }
  if ($y == 0) {
    return $x;
  }

  if ($x > $y) {
    return GCD($x % $y, $y);
  } else {
    return GCD($x, $y % $x);
  }
}

echo GCD(15, 5), PHP_EOL;
